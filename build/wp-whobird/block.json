{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wpwbd/wp-whobird",
  "version": "0.1.0",
  "title": "WhoBIRD observations",
  "category": "widgets",
  "icon": "smiley",
  "description": "Display your WhoBIRD observations",
  "example": {},
  "supports": {
    "html": false
  },
  "textdomain": "wp-whobird",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "viewScript": "file:./view.js",
  "attributes": {
    "periodNumber": {
      "type": "number",
      "default": 1
    },
    "periodUnit": {
      "type": "string",
      "default": "day"
    }
  }
}